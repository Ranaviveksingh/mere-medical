{
  "$schema": "./node_modules/nx/schemas/workspace-schema.json",
  "version": 2,
  "projects": {
    "api": "apps/api",
    "cerner": {
      "root": "libs/cerner",
      "sourceRoot": "libs/cerner/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/cerner/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
          "options": {
            "jestConfig": "libs/cerner/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "docs": {
      "root": "apps/docs"
    },
    "epic": "libs/epic",
    "onpatient": {
      "root": "libs/onpatient",
      "sourceRoot": "libs/onpatient/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/onpatient",
            "main": "libs/onpatient/src/index.ts",
            "tsConfig": "libs/onpatient/tsconfig.lib.json",
            "assets": ["libs/onpatient/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/onpatient/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
          "options": {
            "jestConfig": "libs/onpatient/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "web": "apps/web"
  }
}
